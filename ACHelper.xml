<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>ACHelper configuration file</comment>

    <entry key="platform atcoder">AtCoder</entry>
    <entry key="platform codechef">CodeChef</entry>
    <entry key="platform codeforces">Codeforces</entry>
    <entry key="platform csacademy">CS Academy</entry>
    <entry key="platform facebook">Facebook Hacker Cup</entry>
    <entry key="platform gcj">Google Code Jam</entry>
    <entry key="platform hackerearth">HackerEarth</entry>
    <entry key="platform hackerrank">HackerRank</entry>
    <entry key="platform kattis">Kattis</entry>
    <entry key="platform rcc">Russian Code Cup</entry>
    <entry key="platform timus">Timus</entry>
    <entry key="platform usaco">USACO</entry>
    <entry key="platform yandex">Yandex Algorithm</entry>

    <entry key="shortcut new contest">ctrl C</entry>
    <entry key="shortcut new problem">ctrl P</entry>
    <entry key="shortcut workspace apply changes">ctrl A</entry>
    <entry key="shortcut workspace close problems">ctrl D</entry>
    <entry key="shortcut workspace switch">ctrl W</entry>
    <entry key="shortcut test case new">ctrl T</entry>
    <entry key="shortcut test case edit">ctrl E</entry>
    <entry key="shortcut test case delete">DELETE</entry>
    <entry key="shortcut test case save">ctrl S</entry>
    <entry key="shortcut exit">ctrl Q</entry>

    <entry key="CHelper port">4243</entry>

    <entry key="problem directory">@problem_id@</entry>
    <entry key="default time limit">2</entry>
    <entry key="default memory limit">256</entry>
    <entry key="default checker compiler options">-std=c++11 -O2 -DEPSILON=1e-9 -DABSOLUTE -DRELATIVE</entry>

    <entry key="test sample">sample</entry>
    <entry key="test manual">manual</entry>
    <entry key="test list file">tests</entry>
    <entry key="test maximum loaded length">10000</entry>
    <entry key="test maximum displayed length">100</entry>
    <entry key="test maximum displayed lines">8</entry>

    <entry key="extension input">in</entry>
    <entry key="extension expected output">ans</entry>
    <entry key="extension program output">out</entry>

    <entry key="problems delimiter"># problems</entry>
    <entry key="generate files template">templates/cpp/CMakeScripts.txt</entry>
    <entry key="generate files command">cmake -P CMakeScripts.txt</entry>
    <entry key="generate project template">templates/cpp/CMakeLists.txt</entry>
    <entry key="generate project command">sh project.sh</entry>
</properties>
